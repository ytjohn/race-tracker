{
  "name": "poc-racetracker",
  "version": "1.0.0",
  "description": "**Offline-first, browser-based race tracking system for amateur radio operators supporting trail races.**",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
